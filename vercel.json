{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "pip install -r requirements.txt && npm install",
  "framework": "nextjs",
  "functions": {
    "python/*.py": {
      "runtime": "@vercel/python@4.7.1"
    }
  },
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/mccabe-thiele", "dest": "/api/mccabe-thiele" }
  ]
}
